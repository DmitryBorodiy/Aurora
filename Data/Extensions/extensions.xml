            ExtensionVideoSearchRequest="https://www.google.com/search?q={0}&tbm=vid"
            ExtensionPlacesSearchRequest="https://www.google.com/maps?q={0}"
	    AuthUrl=""
	    IsSignInNeeded="False"
            SubscribitionNeeded="False" />
        <extension 
            ExtensionName="Bing"
            ExtensionType="AllTypes"
            ExtensionDeveloper="Microsoft"
            ExtensionIcon="https://img.icons8.com/fluency/48/null/bing--v1.png"
            ExtensionDescription="Bing web search and other Microsoft apps features."
