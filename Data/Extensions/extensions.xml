            ExtensionVideoSearchRequest="https://www.bing.com/videos/search?q={0}"
            ExtensionPlacesSearchRequest="https://www.bing.com/maps?q={0}"
	    AuthUrl=""
	    IsSignInNeeded="False"
	    SubscribitionNeeded="False" />
    </extensionsList>
    <extensionsList extensionType="photosSearch">
        <extension 
            ExtensionName="Google Photos"
            ExtensionType="SearchPhotos"
            ExtensionDeveloper="Google Inc"
            ExtensionIcon="https://img.icons8.com/fluency/48/null/google-photos-new.png"
            ExtensionDescription="Search in Google Photos"
            ExtensionJSCore=""
            ExtensionWebSearchRequest="https://photos.google.com/search/{0}"
            ExtensionImageSearchRequest=""
            ExtensionNewsSearchRequest=""
            ExtensionVideoSearchRequest=""
            ExtensionPlacesSearchRequest=""
