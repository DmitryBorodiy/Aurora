<!-- type:Extensions -->
<?xml version="1.0" encoding="utf-8" ?>
<extensionsStore>
    <extensionsList extensionType="webSearch">
        <extension 
            ExtensionName="Google"
            ExtensionDeveloper="Google Inc"
            ExtensionIcon="https://img.icons8.com/fluency/48/null/google-logo.png"
            ExtensionDescription="Google web search features for Aurora."
            ExtensionJSCore="https://raw.githubusercontent.com/DmitryBorodiy/Aurora/main/Data/Extensions/js/google.extension.core.js"
            ExtensionWebSearchRequest="https://www.google.com/search?q={0}"
            ExtensionImageSearchRequest="https://www.google.com/search?q={0}&source=lnms&tbm=isch&sa=X&ved=2ahUKEwiO7Ziznv_8AhWPgf0HHYn6Ch4Q_AUoAnoECAEQBA&biw=1536&bih=714"
            ExtensionNewsSearchRequest="https://www.google.com/search?q={0}&source=lnms&tbm=nws"
            ExtensionVideoSearchRequest="https://www.google.com/search?q={0}&tbm=vid"
            ExtensionPlacesSearchRequest="https://www.google.com/maps?q={0}" />
        <extension 
            ExtensionName="Bing"
            ExtensionDeveloper="Microsoft"
            ExtensionIcon="https://img.icons8.com/fluency/48/null/bing--v1.png"
            ExtensionDescription="Bing web search and other Microsoft apps features."
            ExtensionJSCore="https://raw.githubusercontent.com/DmitryBorodiy/Aurora/main/Data/Extensions/js/bing.extension.core.js"
            ExtensionWebSearchRequest="https://www.bing.com/search?q={0}"
            ExtensionImageSearchRequest="https://www.bing.com/images/search?q={0}"
            ExtensionNewsSearchRequest="https://www.bing.com/news/search?q={0}"
            ExtensionVideoSearchRequest="https://www.bing.com/videos/search?q={0}"
            ExtensionPlacesSearchRequest="https://www.bing.com/maps?q={0}" />
    </extensionsList>
    <extensionsList extensionsList="videoSearch">

    </extensionsList>
</extensionsStore>

